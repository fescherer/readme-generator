{
  "editor.wordWrap": "on",
  "errorLens.gutterIconsEnabled": true,
  "colorize.languages": ["typescript", "javascript", "css", "scss"],
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#090909",
    "titleBar.activeForeground": "#0855fa",
    "titleBar.inactiveBackground": "#090909",
    "titleBar.inactiveForeground": "#0855fa",
    "sash.hoverBorder": "#090909"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "css.lint.unknownAtRules": "ignore",
  "eslint.validate": ["javascript"],
  "prettier.enable": false,
  "cSpell.enabled": false
}
